<?xml version="1.0" encoding="UTF-8" ?>   
    <!DOCTYPE mapper   
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 不使用namespace的话sql搜索定位会比较方便-->
<mapper namespace="cn.kw.express.mapper.NetworkMapper">

     <!-- 实体类与数据库映射字段部分  start -->
	<resultMap id="ResultMapNetwork" type="cn.kw.express.pojo.Network">
        <result property="id" column="id"  jdbcType="INTEGER"/>
        <result property="name" column="name"  jdbcType="VARCHAR"/>
        <result property="phone" column="phone"  jdbcType="VARCHAR"/>
        <result property="address" column="address"  jdbcType="VARCHAR"/>
        <result property="lng" column="lng"  jdbcType="DOUBLE"/>
        <result property="lat" column="lat"  jdbcType="DOUBLE"/>
        <result property="companyId" column="companyId"  jdbcType="INTEGER"/>
	</resultMap>
	 <!-- 实体类与数据库映射字段部分  end -->
	 
	 <!-- 声明数据库字段 -->
	<sql id="Network_field">
           id,
           name,
           phone,
           address,
           lng,
           lat,
           companyId
	</sql>
	<sql id="Network_inserts">
		name,
		phone,
		address,
		lng,
		lat,
		companyId
	</sql>
	<!-- 实体类属性  -->
	<sql id="Network_insert">
        	#{name} ,
        	#{phone} ,
        	#{address} ,
        	#{lng} ,
        	#{lat} ,
        	#{companyId} 
	</sql>
	
	<!-- 执行修改时 数据库字段和表字段比较 -->
	<sql id="Network_update">
		    <if test="name != null and name  != ''" >
	            name = #{name} ,
	        </if>
		    <if test="phone != null  and phone  != ''" >
	            phone = #{phone} ,
	        </if>
		    <if test="address != null  and address  != ''" >
	            address = #{address} ,
	        </if>
		    <if test="lng != null" >
	            lng = #{lng} ,
	        </if>
		    <if test="lat != null" >
	            lat = #{lat} ,
	        </if>
		    <if test="companyId != null" >
	            companyId = #{companyId} 
	        </if>
	</sql>
	
	<!-- 查询时条件 -->
	<sql id="Network_where">
		<if test="name != null and name  != ''" >
			and name = #{name}
		</if>
		<if test="phone != null  and phone  != ''" >
			and phone = #{phone}
		</if>
		<if test="address != null  and address  != ''" >
			and address = #{address}
		</if>
		<if test="lng != null" >
			and lng = #{lng}
		</if>
		<if test="lat != null" >
			and lat = #{lat}
		</if>
		<if test="companyId != null" >
			and companyId = #{companyId}
		</if>
	</sql>
	
   <!-- **************************************分割线************************************************************ -->	
	
	<!-- 以下是sql执行语句 
	     注意：1)需要传参数的请注意你使用的方法是 xxByMap还是ByEntuty 存放的参数类型不一致。
	      2)非自定义的方法已经是basedao注明的。方法名一定要和id相同
	      3)自定义类的方法注意是否和共有方法重名id
	      4)执行sql出现顺序和baseDao方法一致，便于查询请勿随意添加，自定义的sql一律放在自定义sql区域
	 -->
	
	
   <!-- ========================================新增==================================================================== -->
	<!-- 
	 【注意添加序列】
	 1.方法描述：新增数据
	 2.参数：实体类
	 3.返回：主键
	-->
	<insert id="insert" parameterType="cn.kw.express.pojo.Network" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO network (
        <include refid="Network_inserts" />
        ) VALUES (
        <include refid="Network_insert" />       
        )
	</insert>
	
	
	 <!-- ========================================删除==================================================================== -->
	<!-- 
	 1.方法描述：根据主键id删除
	 2.参数：主键id
	-->
	<delete id="deleteById" parameterType="java.lang.Integer">
	 delete from network where id=#{id}
	</delete>
	
	
	<!-- 
	 1.方法描述：根据实体类删除
	 2.参数：主键id
	-->
	<delete id="deleteByEntity" parameterType="cn.kw.express.pojo.Network">
	 delete from network
	  WHERE 1=1
      <include refid="Network_where" />   
	</delete>
	
	
	<!-- 
	 1.方法描述：根据map删除
	 2.参数：主键id
	-->
	<delete id="deleteByMap" parameterType="java.util.HashMap">
	 delete from network
	  WHERE 1=1
      <include refid="Network_where" />   
	</delete>
	
	
	 <!-- ========================================修改==================================================================== -->
	<!-- 
	 1.方法描述：修改数据
	 2.参数：实体类
	-->
	<update id="update" parameterType="cn.kw.express.pojo.Network">
        UPDATE network
        <set>
	        <include refid="Network_update" />
	    </set>
        WHERE 1=1
        	<include refid="Network_where" />        
	</update>
	
	<!-- 
	 1.方法描述：修改数据通过id
	 2.参数：实体类
	 -->
	<update id="updateById" parameterType="cn.kw.express.pojo.Network">
        UPDATE network
        <set>
	        <include refid="Network_update" />
	    </set>
        WHERE 1=1
        	and id=#{id}      
	</update>
	
	
	
	 <!-- ========================================查询列表【不分页】==================================================================== -->
	
	<!-- 
	 1.方法描述：用于listByMap根据map查询
	 2.参数：map
	-->
	<select id="listByMap" resultMap="ResultMapNetwork" parameterType="map">
		select <include refid="Network_field" />
 		from network where 1=1
 		<include refid="Network_where" /> 
	</select>
	
	<!-- 
	 1.方法描述：用于select查询所有
	 2.参数：无参数
	-->
	<select id="listAll" resultMap="ResultMapNetwork">
		select <include refid="Network_field" />
 		from network 
	</select>
	
	
	<!-- 
	 1.方法描述：查询所有实体,根据实体属性值为判断条件查询所有实体，
	 2.参数：Entity
	-->
	<select id="listAllByEntity" resultMap="ResultMapNetwork" parameterType="cn.kw.express.pojo.Network">
		select <include refid="Network_field" />
 		from network where 1=1
 		<include refid="Network_where" /> 
	</select>
	
	
	
	 <!-- ========================================查询单个对象==================================================================== -->
	<!-- 
	 1.方法描述：select查询单个
	 2.参数：id
	-->
	<select id="load" resultMap="ResultMapNetwork" parameterType="java.lang.Integer">
		select 	<include refid="Network_field" />
 		from network 
 		where id=#{id}
	</select>
	
	
	
	<!-- 
	 1.方法描述：select查询单个
	 2.参数：id
	-->
	<select id="getById" resultMap="ResultMapNetwork" parameterType="java.lang.Integer">
		select 	<include refid="Network_field" />
 		from network 
 		where id=#{id}
	</select>
	
	<!-- 
	 1.方法描述：用于getByMap根据map查询
	 2.参数：map
	-->
	<select id="getByMap" resultMap="ResultMapNetwork" parameterType="map">
		select <include refid="Network_field" />
 		from network where 1=1
 		<include refid="Network_where" /> 
	</select>
	
	<!-- 
	 1.方法描述：用于Entity根据Entity查询
	 2.参数：Entity
	 -->
	<select id="getByEntity" resultMap="ResultMapNetwork" parameterType="cn.kw.express.pojo.Network">
		select <include refid="Network_field" />
 		from network where 1=1
 		<include refid="Network_where" /> 
	</select>
	
	
	 <!-- ========================================查询列表【分页】==================================================================== -->
     <!-- 
	 1.方法描述：分页
	 2.参数：map
	 -->
	<select id="findByMap" resultMap="ResultMapNetwork" parameterType="map">
		select <include refid="Network_field" />
 		from network where 1=1
 		<include refid="Network_where" /> 
	</select>
	
     <!-- 
	 1.方法描述：分页
	 2.参数：Entity
	 -->
	<select id="findByEntity" resultMap="ResultMapNetwork" parameterType="cn.kw.express.pojo.Network">
		select <include refid="Network_field" />
 		from network where 1=1
 		<include refid="Network_where" /> 
	</select>
	
	
	 <!-- ========================================批量新增修改==================================================================== -->
	<!-- 
	 1.方法描述：批量插入
	 2.参数：集合/数组
	-->
	<insert id="insertBatch" parameterType="java.util.List">
        INSERT INTO network (
        <include refid="Network_field" />
        ) VALUES 
        <foreach collection="list" item="item" index="index"
			separator=",">
			(
			
	            #{item.name} ,
	            #{item.phone} ,
	            #{item.address} ,
	            #{item.lng} ,
	            #{item.lat} ,
	            #{item.companyId} 
			
			)
		</foreach>       
        
	</insert>
    
    <!-- 
	 1.方法描述：批量修改
	 2.参数：集合/数组
	-->
	<update id="updateBatch" parameterType="java.util.List">
		<foreach collection="list" item="item" index="index"
			separator=";">
        UPDATE 
        network 
        <set>
		    <if test="item.name != null" >
	        name = #{item.name},
	        </if>
		    <if test="item.phone != null" >
	        phone = #{item.phone},
	        </if>
		    <if test="item.address != null" >
	        address = #{item.address},
	        </if>
		    <if test="item.lng != null" >
	        lng = #{item.lng},
	        </if>
		    <if test="item.lat != null" >
	        lat = #{item.lat},
	        </if>
		    <if test="item.companyId != null" >
	        companyId = #{item.companyId}
	        </if>
	    </set>
        WHERE 
        	1=1 
        </foreach>    
	</update>

   <!-- ******************************自定义 sql************************************************************** -->
   
   <!-- 
           通过sql查询单个对象 返回对象
    -->
   <select id="getBySqlReturnEntity" resultMap="ResultMapNetwork">
		${sql}
   </select>
   
    <!-- 
           通过sql查询单个对象 返回map
    -->
   <select id="getBySqlReturnMap" resultType="map">
		${sql}
   </select>
   
   <!-- 
           通过sql查询集合 返回对象
    -->
   <select id="listBySqReturnEntity" resultMap="ResultMapNetwork">
		${sql}
   </select>
   
    <!-- 
           通过sql查询查询集合 返回map
    -->
   <select id="listBySqReturnMap" resultType="map">
		${sql}
   </select>
   
   <!-- 
           通过sql查询分页集合 返回对象
    -->
   <select id="findBySqlRerturnEntity" resultMap="ResultMapNetwork">
		${sql}
   </select>
   
   <!-- 通过sql修改 -->
   <update id="updateBysql">
       ${sql}  
   </update>
   
   <delete id="deleteBySql">
	${sql}  
	</delete>
	
</mapper>

